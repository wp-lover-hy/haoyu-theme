{
  "name": "haoyuai-child-theme",
  "version": "1.0.0",
  "description": "HaoYu AI Child Theme for GeneratePress - Modern Development Environment",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:analyze": "vite build --mode analyze",
    "type-check": "tsc --noEmit",
